function xiazai(path){
	 window.location.href="${ctxPath}/excel/getDocFile?docpath=D:/upload/"+path+"";
}
var dialog1;
function uploadxw(id,lhsl){
	parent.dialog1=BootstrapDialog.show({
	    title: '延长架空期',
	    message:$('<iframe frameborderno="0"  style="width:100%;height:100%;" frameborder="0";  src="${ctxPath}/remote/lunhuanguanli/lengthenjkq.html?id='+id+'"/>'),
	    onshow:function(dlg){
	       	dlg.$modalDialog.css("width","800px");//modal-dialog
	       	$(".bootstrap-dialog-message", dlg.$modalContent).css("height","300px");//modal-dialog
       },
       buttons: [{
           label: '确认',
           action: function(dialog) {
        	   $(window.parent.document).contents().find("iframe")[0].contentWindow.saveycjkq(); 
        	   
           },
          cssClass: 'btn-success btn-xs'
       	},{
           label: '取消',
           action: function(dialog) {
               dialog.close();
           },
          cssClass: 'btn-xs btn-danger'
       }]
	    
	});
}
$("#tb").on("draw.dt",function(){
	$("#hejitd").text(0);
	var q={};
	if($("#qyzzjgdm").val()!=""&&$("#qyzzjgdm").val()!=null){
		q.qyzzjgdm=$("#qyzzjgdm").val();
	}
	if($("#ccku").val()!=""&&$("#ccku").val()!=null){
		q.ccku=$("#ccku").val();
	}
	if($("#ccdian").val()!=""&&$("#ccdian").val()!=null){
		q.ccdian=$("#ccdian").val();
	}
	if($("#pz").val()!=""&&$("#pz").val()!=null){
		q.pz=$("#pz").val();
	}
	if($("#scnd").val()!=""&&$("#scnd").val()!=null){
		q.year=$("#scnd").val();
	}
	if($("#qssj").val()!=""&&$("#qssj").val()!=null){
		q.qssj=$("#qssj").val();
	}
	if($("#jssj").val()!=""&&$("#jssj").val()!=null){
		q.jssj=$("#jssj").val();
	}
	if($("#status").val()!=""&&$("#status").val()!=null){
		q.status=$("#status").val();
	}
	$.ajax({
		url:'${ctxPath}/lunhuan/lcpwheji',
		type:'post',
		data:q,
		dataType:'json',
		success:function(data){
			if(data!=null){
				$("#hejitd").text(data.zonglhsl.toFixed(3));
			}
		}
	});
});
