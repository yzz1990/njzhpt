		url : '${ctxPath}/chubeiliangJH/chengchuku',
		type : 'post',
		data : {
			"yijiqiye" : qyzzjgdm,
		},
		dataType : 'json',
		success : function(data) {
			 for (var i = 0; i < data.length; i++) {
				 if("${obj.ccku}"==data[i].kdbm){
					 $("#ccku").append("<option value="+data[i].kdbm+"  selected>" + data[i].kdmc+ "</option>");
				 }else{
					$("#ccku").append("<option value="+data[i].kdbm+">" + data[i].kdmc+ "</option>");
				 }
			} 
		
			/* $('#kdbm').select2({
			  data:data

			}); */
