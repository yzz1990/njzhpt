            <!-- Row End -->
            
    <script type="text/javascript">

    function savelsscba() {
    	var form = $("#contentform");
    	$.ajax({
    		url: form.attr("action"),
    		type:"post",
    		dataType:"json",
    		data: form.serialize(),
    		success: function(msg) {
    			if (!msg.ret) {
    				alert(msg.message);
    			} else {
    				alert("保存成功");
    			}
    		},
    		error:function(){
    			alert("保存失败,请检查内容是否重复");
    		}
    	});
    }
    
    </script>
            
</body>


</html>