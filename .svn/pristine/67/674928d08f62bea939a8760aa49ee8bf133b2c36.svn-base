//根据点击改变图标的样式
function show(xzqhdm){
	var node = $("#"+xzqhdm);
	var sp= node.find(' > ul > li');
    if (sp.is(":visible")) {
    	sp.hide('fast');
    	node.find(' > span > i').addClass('icon-folder-close').removeClass('icon-folder-open');
    } else {
    	sp.show('fast');
    	node.find(' > span > i').addClass('icon-folder-open').removeClass('icon-folder-close');
    }
}


function showkd(ID){
	var node = $("#"+ID);
	var sp= node.find(' > ul > li');
    if (sp.is(":visible")) {
    	sp.hide('fast');
    	node.find(' > span > i').addClass('icon-folder-close').removeClass('icon-folder-open');
    } else {
    	sp.show('fast');
    	node.find(' > span > i').addClass('icon-folder-open').removeClass('icon-folder-close');
    }
   // alert("ss");
}



