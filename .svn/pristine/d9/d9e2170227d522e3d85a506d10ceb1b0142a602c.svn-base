		    onshow:function(dlg){
		      	dlg.$modalDialog.css("width","1000px");//modal-dialog
		      	$(".bootstrap-dialog-message", dlg.$modalContent).css("height","500px");//modal-dialog
	      },
	      buttons: [
	      	{
	          label: '取消',
	          action: function(dialog) {
	              dialog.close();
	          },
