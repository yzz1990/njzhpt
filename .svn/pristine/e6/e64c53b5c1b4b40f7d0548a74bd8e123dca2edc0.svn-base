<script type="text/javascript">


Array.prototype.S=String.fromCharCode(2);
Array.prototype.in_array=function(e){
    var r=new RegExp(this.S+e+this.S);
    return (r.test(this.S+this.join(this.S)+this.S));
};

var mydate = new Date();
var tyear = mydate.getFullYear();
var tday = mydate.toLocaleDateString();

var ccqyid = [];
var datatable;
var index = 0;
var flag = 0;
var changekd = 0;
var changepz = 0;
$(function(){
	$("#sqwh").val("宁粮储轮["+year+"]");
	$("#liuchenghao,#ccku,#ccdian,#cfbm").select2();
	//获取流程号
	$.ajax({
		 url:'${ctxPath}/lunhuan/findlcbylc',
		 type:'post',
		 data:{},
		 dataType:'json',
		 success:function(data){
			$.each(data.list,function(i, n){
				$("#liuchenghao").append("<option jiedian="+n.jiedian+" value="+n.liuchenghao+">"+n.name+"</option>");
			});
		 }
	});
	//获取承储企业
	$.ajax({
		url:'${ctxPath}/lunhuan/queryCcqy',
		type:'post',
		data:{"xian":"${name}"},
		dataType:'json',
		success:function(data){
			for(var i = 0; i < data.list.length; i++){
				if(!ccqyid.in_array(data.list[i].ccqy)){
					var op = "<option value="+data.list[i].ccqy+">"+data.list[i].qymc+"</option>";
				     $("#ccku").append(op);
				}
				ccqyid.push(data.list[i].ccqy);
			}
		}
	});
	
	//行政区划代码赋值
	$.ajax({
		url:'${ctxPath}/shouchukeshihua/findquxian',
		type:'post',
		data:{"name":"${name}"},
		dataType:'json',
		success:function(data){
			$("#xzqhdm").val(data[0].xzqhdm);
		},
		error:function(){
			alert("error");
		}
	});
	
	//加载一级企业的企业组织机构编码
	$.ajax({
		url:'${ctxPath}/shouchukeshihua/queryqiye.do',
		type:'post',
		data:{"qymc":"${dep}"},
		dataType:'json',
		success:function(data){
			$("#qyzzjgdm").val(data.list[0].qyzzjgdm);
		}
	});
	
	for(var i=0; i < 6; i++){
		var op = '<option value="'+(year-i)+'">'+(tyear-i)+'</option>';
		$("#year").append(op);
	}
	
	$('#sqsj').datetimepicker({
		minView: "month",
		format:'yyyy-mm-dd',
		language: 'zh-CN',
		autoclose: 1,
		startView: 2,
	});
	$("#sqsj").val(tday);
	$('#sqsj_tijiao').datetimepicker({
		minView: "month",
		format:'yyyy-mm-dd',
		language: 'zh-CN',
		autoclose: 1,
		startView: 2,
	});
	$('#sqsj_tijiao').val(tday);
	
});

function loadqiye(index){
	//获取承储企业
	var qiyeid = [];
	$.ajax({
		url:'${ctxPath}/lunhuan/queryCcqy',
		type:'post',
		data:{"xian":"${name}"},
		dataType:'json',
		success:function(data){
			for(var i = 0; i < data.list.length; i++){
				if(!qiyeid.in_array(data.list[i].ccqy)){
					var op = "<option value="+data.list[i].ccqy+">"+data.list[i].qymc+"</option>";
				     $("#qiye"+index).append(op);
				}
				qiyeid.push(data.list[i].ccqy);
			}
		}
	});
}



/**
 * 添加轮出批复
 */
function appendlcpf(){
	var qyzzjgdm = $("#ccku option:selected").val();
	var kdbm = $("#ccdian option:selected").val();
	if(qyzzjgdm == null || qyzzjgdm == "" || kdbm == null || kdbm == ""){
		alert("承储库和承储点不能为空");
		return false;
	}
	flag++;
	index++;
	var linshi=
	'<tr id="tr'+index+'">'+
		'<td><select id="cfbm'+index+'" class="cfbm" style="width:100%;height:100%" onchange="getcfbm(\''+index+'\')"><option value="">请选择</option></select></td>'+
		'<td><input id="cflhsl'+index+'" class="cflhsl"  type="text"  style="width:100%;height:100%" onkeyup="this.value=this.value.replace(/\D/gi,\'\')" onafterpaste="this.value=this.value.replace(/\D/g,\'\')" /></td>'+
		'<td  style="text-align: center";><button type="button" class="btn btn-xs btn-success"onclick="shanchu(\''+index+'\')"><i class="fa fa-pencil" aria-hidden="true">&nbsp删除</i></button></td>'+
	'</tr>';
	loadcfbm(index);
	$("#cftable").append(linshi);
	
}

	function shanchu(index){
		$("#tr"+index).remove();
		flag--;
	}
	
	var zongkuc;
	function shangchuan(){
		if(flag == 0){
			alert("请添加轮入明细");
			return false;
		}
		var cfs = "";
		var cflrsl = "";
		for(var i=1;i<=index;i++){
			if($("#cfbm"+i).length>0){
				if(""==$("#cfbm"+i+" option:selected").val() 
						|| "请选择"==$("#cfbm"+i+" option:selected").val()
						|| null==$("#cfbm"+i+" option:selected").val()){
					alert("请选择仓房名称！");
					return false;
				}
				cfs += $("#cfbm"+i).val()+",";
				if(""==$("#cflhsl"+i).val() || null==$("#cflhsl"+i).val()){
					alert("填写轮入数量明细！");
					return false;
				}
				cflrsl += $("#cflhsl"+i).val()+",";
			}
		}
		
		$("#cfs").val(cfs);
		$("#cflrsl").val(cflrsl);
		$("#changekd").val(changekd);
		$("#changepz").val(changepz);
		if($("#liuchenghao option:selected").val()=="" 
				|| $("#liuchenghao option:selected").val()==null
				|| $("#liuchenghao option:selected").val()=="请选择"){
			alert("请选择流程号 ");
			return false;
		}else if($("#ccku option:selected").val()=="" 
				|| $("#ccku option:selected").val()==null
				|| $("#ccku option:selected").val()=="请选择"){
			alert("请选择承储企业 ");
			return false;
		}else if($("#avgprice").val()=="" 
			|| $("#avgprice").val()==null){
			alert("请填写平均轮入价格");
			return false;
		}else if($("#ccdian option:selected").val()==""
				|| $("#ccdian option:selected").val()==null
				|| $("#ccdian option:selected").val()=="请选择"){
			alert("请选择承储库 ");
			return false;
		}else if($("#pz option:selected").val()==""
				|| $("#pz option:selected").val()==null
				|| $("#pz option:selected").val()=="请选择"){
			alert("请选择品种名称 ");
			return false;
		}else if($("#sqwh").val()=="" 
				|| $("#sqwh").val()==null){
			alert("请输入申请文号 ");
			return false;
		}else if($("#lhsl").val()=="" || $("#lhsl").val()==null){
			alert("请输入轮入数量 ");
			return false;
		}else if($("#sqsj").val()=="" || $("#sqsj").val()==null){
			alert("请输入轮入完成时间 ");
			return false;
		}else if($("#sqsj_tijiao").val()=="" || $("#sqsj_tijiao").val()==null){
			alert("请输入申请验收时间 ");
			return false;
		}else {
			$.ajax({
				url:'${ctxPath}/lunhuan/queryLunRuShenQing.do',
				type:'post',
				dataType:'json',
				success:function(data){
					var sqwhs = [];
					var sqwh = $("#sqwh").val();
					var flag = true;
					$.each(data.list,function(i, n){
						if(sqwh == n.sqwh){
							flag = false;
						}
					});
					if(flag == true){
						$.ajax({
							url: "${ctxPath}/lunhuan/saveLunRu.do",
							dataType: "json",
							data:$("#sc").serialize(),
							success: function(msg) {
								if (msg.ret) {
								  	parent.dialog1.close();
							   		alert("轮入申请成功");
							   		parent.window.datatable.ajax.reload(null,true);
								}else{
							   		alert(ret.message);
								}
								
								var qyzzjgdm = $("#ccku option:selected").val();
								var kdbm = $("#ccdian option:selected").val();
								var kdmc = $("#ccdian option:selected").text();
								var pz = $("#pz option:selected").val();
								var sumlunru = $("#lhsl").val();
								
								if(changekd == 1){		//变更库点
									qyzzjgdm = $("#kudian option:selected").attr("qyzzjgdm");
									kdbm = $("#kudian option:selected").attr("kdbm");
									kdmc = $("#kudian option:selected").text();
								}
								if(changepz == 1){	//变更品种
									pz = $("#bgpz option:selected").val();
								}
								
								//保存轮入总数
								$.ajax({
									url:'${ctxPath}/lunhuan/saveSumlunru.do',
									type:'post',
									data:{"qyzzjgdm":qyzzjgdm,"kdbm":kdbm,"kdmc":kdmc,"pz":pz,"sumlunru":sumlunru},
									dataType:'json',
									success:function(data){
									},
									error:function(){
										
									}
								});
								
								
								
							}
						}); 
					}else{
						alert("申请文号重复请重新填写");
						return false;
					}
				},
				error:function(){
					
				}
			});	
		}
	}
	
	
	//企业下拉框值改变时 联动查询库点名称
	function selectKDMC(){
		$("#lhsq_kdmc").html("");
		var xian = $("#szqy").val();
		var qymc=$("#qymc").val();
		var qyzzjgdm="";
		//给xzqhdm赋值（时间传到后台值）
		$.ajax({
			url:'${ctxPath}/shouchukeshihua/findquxian',
			type:'post',
			data:{"name":xian},
			dataType:'json',
			success:function(data){
				$("#xzqhdm").val(data[0].xzqhdm);
			},
			error:function(){
				alert("error");
			}
		});
		
		$.ajax({
			url:'${ctxPath}/shouchukeshihua/queryqiye.do',
			type:'post',
			data:{"qymc":qymc},
			dataType:'json',
			success:function(data){
				qyzzjgdm=data.list[0].qyzzjgdm;
				$("#qymcc").val(qyzzjgdm);
			
				//选完企业后 自动显示该企业下的储备粮计划和实物库存（取该企业下的所有...）
				$.ajax({
