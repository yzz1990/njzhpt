		 type:'post',
		 data:{},
		 dataType:'json',
		 success:function(data){
			$.each(data.list,function(i, n){
				if("${obj.liuchenghao}"==n.liuchenghao){
					$("#liuchenghao").append("<option value='"+n.liuchenghao+"' selected >"+n.name+"</option>");
				}else{
					$("#liuchenghao").append("<option value='"+n.liuchenghao+"'>"+n.name+"</option>");
				}
			});
			getliucheng();
		 }
	});
	//整仓轮出
			$.ajax({
				url:'${ctxPath}/lunhuan/findYearPZ.do',
				type:'post',
				data:{"qyzzjgdm":"${obj.ccku}","kdbm":"${obj.ccdian}","cfbm":"${obj.cfbm}"},
				dataType:'json',
				success:function(data){
						 if(null!=data.ret){
							 zongkuc=data.ret.zongkucun;
						 }
