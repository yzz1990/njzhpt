//获取当前周数
function getYearWeek(date){
	var date2=new Date(date.getFullYear(), 0, 1);
	var day1=date.getDay();
	if(day1==0) day1=7;
	var day2=date2.getDay();
	if(day2==0) day2=7;
	d = parseInt((date.getTime() - date2.getTime()+(day2-day1)*(24*60*60*1000)) / 86400000);
	return parseInt(d /7)+1;
}


//select改变事件
function Change(){
	//清楚下拉框
	$("#nianzs").empty();
	//获取年度
	var niandu = $("#niandu option:selected").text();
	//生成星期数据
	$.ajax({
		url:'${ctxPath}/jiage/findweek',
		type:'post',
		data:{"niandu":niandu},
		dataType:'json',
		success:function(data){
			//初始化采价点下拉框
		    //$(".nianzs-div").find(".select2-selection__rendered").text("请选择");
			//$(".nianzs-div").find(".select2-selection__rendered").attr("title","请选择");
			//$("#nianzs").append('<option value="请选择">请选择</option>');
			for(var i = 0; i < data.length; i++){	
				//var op = '<option value="'+(i+1)+'">'+data[i]+'</option>';
				//$("#nianzs").append(op);
				
				if(niandu==new Date().getFullYear())
				{
					if(i<getYearWeek(new Date())-1)
					{
						var op = '<option value="'+(i+1)+'">'+data[i]+'</option>';
						$("#nianzs").prepend(op);
					}
				}else{
					var op = '<option value="'+(i+1)+'">'+data[i]+'</option>';
					$("#nianzs").prepend(op);
				}
				
				
			}
			
		},
		error:function(){
			alert("生成年周数失败");
		}
	});
}

