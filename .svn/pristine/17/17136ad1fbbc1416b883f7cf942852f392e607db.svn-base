$("#shiwukucuntb").on("draw.dt",function(){
	$(".details-control").css("cursor","pointer");
	var pinzhong=$("#pinzhong").val();
	var erjiqiyekd;
	if(""!=$("#kdbm").val()&&$("#kdbm").val()!=null){
		erjiqiyekd=$("#kdbm").val();
	}
	//加载合计方法
	$.ajax({
		url: "${ctxPath}/chubeiliangJH/shiwukucunheji",
		dataType: "json",
		data:{"year":$("#year").val(),"pinzhong":pinzhong,"yijiqiye":$("#qyzzjgdm").val(),"erjiqiyekd":erjiqiyekd,"month":$("#month").val(),"xingzhi":$("#xingzhi").val(),"page":1,"rows":10000,"diqu":"${array.contain(session.userinfo.auth,'auth_county')?session.userinfo.area.name:''}"},
		success: function(data) {
		//alert(data.cbljh_xm);
		var xj;
		var xm;
		var jd;
		var xd;
		var shijixj;
		var shijixm;
		var shijijd;
		var shijixd;
		var xjzb;
		var xmzb;
		var jdzb;
		var xdzb;
		var xjzbbool;
		var xmzbbool;
		var jdzbbool;
		var xdzbbool;
		if(data==""||data==null){
			xj=0;
			xm=0;
			jd=0;
			xd=0;
			shijixj=0;
			shijixm=0;
			shijijd=0;
			shijixd=0;
			xjzb="";
			xmzb="";
			jdzb="";
			xdzb="";
			xjzbbool="1";
			xmzbbool="1";
			jdzbbool="1";
			xdzbbool="1";
		}else{
			xj=data.xj.toFixed(0);
			xm=data.xm.toFixed(0);
			jd=data.jd.toFixed(0);
			xd=data.xd.toFixed(0);
			shijixj=data.shijixj.toFixed(0);
			shijixm=data.shijixm.toFixed(0);
			shijijd=data.shijijd.toFixed(0);
			shijixd=data.shijixd.toFixed(0);
			xjzb=data.xjzb;
			xmzb=data.xmzb;
			jdzb=data.jdzb;
			xdzb=data.xdzb;
			xjzbbool=data.xjzbbool;
			xmzbbool=data.xmzbbool;
			jdzbbool=data.jdzbbool;
			xdzbbool=data.xdzbbool;
		}
		$("#hejixj").text(xj);
		$("#hejixm").text(xm);
		$("#hejijd").text(jd);
		$("#hejixd").text(xd);
		$("#hejishijixj").text(shijixj);
		$("#hejishijixm").text(shijixm);
		$("#hejishijijd").text(shijijd);
		$("#hejishijixd").text(shijixd);
		if("1"==xjzbbool){
			$("#hejixjzb").text(xjzb);
		}else{
			$("#hejixjzb").html("<font style='color:red;font-size:16px;'>"+xjzb+"</font>");
		}
		if("1"==xmzbbool){
			$("#hejixmzb").text(xmzb);
		}else{
			$("#hejixmzb").html("<font style='color:red;font-size:16px;'>"+xmzb+"</font>");
