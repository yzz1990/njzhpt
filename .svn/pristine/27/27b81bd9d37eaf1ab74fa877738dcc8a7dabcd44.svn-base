	<script type="text/javascript">
	Array.prototype.S=String.fromCharCode(2);
	Array.prototype.in_array=function(e){
	    var r=new RegExp(this.S+e+this.S);
	    return (r.test(this.S+this.join(this.S)+this.S));
	};
	var kudianmc;
	var indexry=1;
	var indexcf=1;
	var index=1;
	$(document).ready(function() {
	  $(".js-example-basic-single").select2();
	  $("#liuchenghao").select2();
	  kudianmc = $("#kudianmc").val;
	});
	
	$(function(){
	$("#sqrq").datetimepicker({
			
			format : 'yyyy-mm-dd',
			language : 'zh-CN', //这里需要修改
			weekStart : 1,
			autoclose : 1,
			todayHighlight : 1,
			startView : 2,
			minView : 2,
			forceParse : 0
		});
		if("${obj.fujianname}"==null||"${obj.fujianname}"==""){
			$("#seefujian").attr("href","javascript:void(0)");
			$("#seefujian").removeAttr("download");
		}
		if("${obj.status}".substring(1,2)=="0"){
			$("#status").val("1");
		}
		if($("#status").val()!="1"){
			$("#seefujian").css("visibility","visible");
			$("#shangchuan").css("display","none");
			$("#beianbianhao").attr("readonly",true);
			$("#qiyeid").attr("disabled",true);
			$("#liuchenghao").attr("disabled",true);
			$("#kudian").attr("disabled",true);
			$("#shuliang").attr("readonly",true);
			$("#fuzeren").attr("readonly",true);
			$("#fangshi").attr("disabled",true);
			$("#remark").attr("readonly",true);
			
			
			
		}
		if("${obj.id}"!=""){
			var ri="${obj.beianriqi}";
			var riqi=ri.split(" ");
			$("#liuchengtu").load("${ctxPath}/renyuan/liuchengtuxz?liuchenghao=${obj.liuchenghao}&qiyeid=${obj.qiyeid}&nyr="+riqi[0]+"&sfm="+riqi[1]+"");
		}
		
		$('.file_upload').uploadifive({
			'auto' : true,   //自动上传 
	        'uploadScript' : '${ctxPath}/excel/uploadlhpzxwFile',  //处理上传文件Action路径
	        'buttonClass' : 'file_upload',
	        'fileObjName' : 'file',        //文件对象
	  	    'buttonText'   : '上传附件',   //按钮显示文字 
	  	 	'queueID'      : 'tip-queue', //提示信息放置目标 
	  	    'onUploadComplete' : function(file, data) { //文件上传成功后执行
		    	var obj = JSON.parse(data);
		    	$("#scfilename").html(obj.scfilename);
		    	$("#fujianname").val(obj.scfilename);
		    	//alert(obj.filepath);
		    	$("#scfilename").attr("href","${ctxPath}"+obj.filepath);
		    	$("#scfilename").attr("download",obj.scfilename);
		    	$("#fujianpath").val(obj.filepath);
	    	}
		});
		/* 获取企业名称 */
		$.ajax({
			url:'${ctxPath}/renyuan/findQiye',
			type:'post',
			data:{"id":"${session.userinfo.user.id}"},
			dataType:'json',
			success:function(data){
				$("#qiyeid").append("<option value=''>请选择</option>");
				$.each(data.list,function(i,n){
					if("${obj.qiyeid}"==n.ID){
						$("#qiyeid").append("<option qydm="+n.qyzzjgdm+" value="+n.ID+" selected>"+n.qymc+"</option>");
					}else{
						$("#qiyeid").append("<option qydm="+n.qyzzjgdm+" value="+n.ID+">"+n.qymc+"</option>");
					}
					
				});
				huoquXz();
				huoquYjxinxi();
			}
		});
		
		/*作业人员获取*/
		
		$.ajax({
			url:'${ctxPath}/renyuan/findzuoyerenyuan',
			type:'post',
			data:{"page":1,"rows":1000,"beianbianhao":"${obj.beianbianhao}"},
			dataType:'json',
			success: function(data) {
				
				 $.each(data.list,function(i, n){
					 if(data.list.length-1!=i){
						 indexry++;
						 var text="<tr><td>"+indexry+"</td>"
							+'<td><input id="xingming'+indexry+'"  style="width: 100%;height: 100%;border: 0"></td>'
							+'<td><input id="zz'+indexry+'"  style="width: 100%;height: 100%;border: 0"></td>'
							+'</tr>'
							$("#firstry").append(text);	
						 $("#xingming"+(indexry-1)).val(n.xingming);
						 $("#zz"+(indexry-1)).val(n.zz);
					 }else{
						 $("#xingming"+(indexry)).val(n.xingming);
						 $("#zz"+(indexry)).val(n.zz);
					 }
					 
									 
				 });
			},
			error:function(){
				//alert("error");
			}
		});
		function getXz(cfmc,indexcf){
			var qyzzjgdm=$("#qiyeid option:selected").attr("qydm");
			 var kdbm=$("#kudian option:selected").val();
			$.ajax({
				url: "${ctxPath}/lunhuan/selectCH.do",
				dataType: "json",
				data:{kdbm:kdbm,qyzzjgdm:qyzzjgdm},
				success: function(data) {
					$.each(data.list,function(i, k){
								//alert(n.cfmc+"=="+k.cfbm)											
								if(cfmc==k.cfbm){
									$("#cfmc"+(indexcf-1)).append("<option value="+k.cfbm+" selected>"+k.cfmc+"</option>");	
								}else{
									$("#cfmc"+(indexcf-1)).append("<option value="+k.cfbm+" >"+k.cfmc+"</option>");						
								}
								
																				
					});
				}
			});	
		}
		function getXzYj(cfmc,index){
			var qyzzjgdm=$("#qiyeid option:selected").attr("qydm");
			 var kdbm=$("#kudian option:selected").val();
			$.ajax({
				url: "${ctxPath}/lunhuan/selectCH.do",
				dataType: "json",
				data:{kdbm:kdbm,qyzzjgdm:qyzzjgdm},
				success: function(data) {
					$.each(data.list,function(i, k){
								//alert(n.cfmc+"=="+k.cfbm)											
								if(cfmc==k.cfbm){
									$("#cfmcyj"+(index-1)).append("<option value="+k.cfbm+" selected>"+k.cfmc+"</option>");	
								}else{
									$("#cfmcyj"+(index-1)).append("<option value="+k.cfbm+" >"+k.cfmc+"</option>");						
								}
								
																				
					});
				}
			});	
		}
		//获取药剂名称
		function getXzyjmc(yjmc,pp,gg,dw,sccj,index,lysl,syl){
			
		var ch=[];
		$.ajax({
			url:'${ctxPath}/renyuan/findyjkcxinxi',
			type:'post',
			data:{"page":1,"rows":1000},
			dataType:'json',
			success: function(data) {
				$.each(data.list,function(i, n){
					if(!ch.in_array(n.yjmc)){
						if(yjmc==n.yjmc){
							$("#yaojim"+(index-1)).append("<option value="+n.yjmc+" selected>"+n.yjmc+"</option>");	
							getpp2(pp,gg,dw,sccj,index-1,lysl,syl);
						}else{
							$("#yaojim"+(index-1)).append("<option value="+n.yjmc+">"+n.yjmc+"</option>");	
						}
							
					}
					ch.push(n.yjmc);
					
				});
			},
			error:function(){
				//alert("error");
			}
		});
		}
		//var indexcf=1;
