//生成报告
function yulan(){
	if(""==$("#lhsl").val()||null==$("#lhsl").val()){
		alert("请先完善轮换申请信息！");
		return false;
	}
	//品种
	var data = {};
	data.pz = $("#pz option:selected").text();
	//企业名称
	data.qymc = $("#ccku option:selected").text();
	//库点名称
	data.kdmc = $("#ccdian option:selected").text();
	//轮入数量
	data.lhsl = $("#lhsl").val();
	//平均价格
	data.avgprice = $("#avgprice").val();
	//仓房和数量
	var cfbmlr = "";
	for(var i=1;i<=index;i++){
		if($("#cfbm"+i).length>0){
			if(""==$("#cfbm"+i+" option:selected").val() 
					|| "请选择"==$("#cfbm"+i+" option:selected").val()
					|| null==$("#cfbm"+i+" option:selected").val()){
				alert("请选择仓房名称！");
				return false;
			}
			if(""==$("#cflhsl"+i).val() || null==$("#cflhsl"+i).val()){
				alert("填写轮入数量明细！");
				return false;
			}
			cfbmlr += $("#cfbm"+i).val()+"号仓"+$("#cflhsl"+i).val()+"吨,";
		}
	}
	data.cfbmlrs = cfbmlr;
	//现在时间
	var ndate = new Date;
	var nyear = parseInt(ndate.getFullYear());
	var nmonth = parseInt(ndate.getMonth()) + 1;
	var nday = parseInt(ndate.getMonth());
	data.nyear = nyear;
	data.nmonth = nmonth;
	data.nday = nday;
	//以下时间暂定
	data.sqyear = data.nyear;
	data.sqmonth = data.nmonth;
	data.pzyear = data.nyear;
	data.pzmonth = data.nmonth;
	data.pzday = data.nday;
	//申请文号
	data.sqwh = $("#sqwh").val();
	
	$.ajax({
		url : '${ctxPath}/excel/yulanlrsq',
		type : 'post',
		data : data,
		dataType : 'json',
		success : function(data) {
			if(data.ret){
				$('#xzbtn').removeAttr("disabled");
				$("#sqdocpath").val(data.docname);
				$("#sqdocname").html(data.docname);
			}
			/* $('#kdbm').select2({
			  data:data

			}); */
		}
	});
}

	</script>
