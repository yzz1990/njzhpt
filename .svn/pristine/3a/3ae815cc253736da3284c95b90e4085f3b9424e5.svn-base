//审批到下一个
function shenpi(){
	var pzwh = $("#pzwh").val();
	var pzsj = $("#pzsj").val();
	var lunruimg = $("#lunruimg").val();
	var bgkdimg = $("#bgkdimg").val();
	var bgpzimg = $("#bgpzimg").val();
	if(pzwh == "" || pzwh == null){
		alert("请输入批复文号");
		return false;
	}
	if(pzsj == "" || pzsj == null){
		alert("请输入批复时间");
		return false;
	}
	
	$.ajax({
		url:'${ctxPath}/lunhuan/lunrushenpi.do',
		type:'post',
		data:{"sqwh":"${sqwh}","pzwh":pzwh,"pzsj":pzsj,"lunruimg":lunruimg,"bgpzimg":bgpzimg,"bgkdimg":bgkdimg},
		dataType:'json',
		success:function(data){
			if(data.ret){
				parent.dialog1.close();
		   		alert("轮入申请成功");
		   		parent.window.datatable.ajax.reload(null,true);
			}else{
				alert("审批失败");
			}
		},
		error:function(){
		}
	});
}

//驳回申请
function bohui(){
	$.ajax({
		url:'${ctxPath}/lunhuan/lunrubohui.do',
		type:'post',
		data:{"sqwh":"${sqwh}"},
		dataType:'json',
		success:function(data){
			if(data.ret){
				parent.dialog1.close();
		   		alert("驳回成功");
		   		parent.window.datatable.ajax.reload(null,true);
			}else{
				alert("驳回失败");
			}
		},
		error:function(){
			
		}
	});
}

