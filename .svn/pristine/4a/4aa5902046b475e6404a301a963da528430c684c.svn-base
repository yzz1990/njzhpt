            		var pifu = '<button type="button" onclick="xiazai(\''+row["docpath"]+'\')"  style="" class="btn btn-xs btn-success"><i class="fa fa-pencil" aria-hidden="true"></i>&nbsp;查看批复</button>';
      				return baogao+"  "+pifu;
            	}
            }]
