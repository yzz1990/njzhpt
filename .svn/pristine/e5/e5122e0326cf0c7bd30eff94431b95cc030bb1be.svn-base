				}
			},{
				"render": function (data,type,row,meta) {
					var zcr = Math.round(row["zcr"]);
					var div = '<div class="">'+zcr+'</div>';
					return div;
				}
			},{
				"render": function (data,type,row,meta) {
					var kucun = Math.round(row["sum"]);
					kucuns.push(kucun);
					var div = '<div class="">'+kucun+'</div>';
					return div;								
				}
			},{
				"render": function (data,type,row,meta) {
					var kongc = Math.round(row["kongc"]);
					kongcs.push(kongc);
					var div = '<div class="">'+kongc+'</div>';
					return div;								
				}
			},{
				"render": function (data,type,row,meta) {
					var qt = Math.round(row["qt"]);
					qts.push(qt)
					var div = '<div class="">'+qt+'</div>';
					return div;								
				}
			}
			]
	});	
	$('#zycr').on( 'draw.dt', function () {
		getxian(0);
		
	});
	
	
	
});


/**
 * 获取区县饼图
 */
function getxian(orderno){
	var leixin = ["库存","空仓","其它"];
	var qu = xians[orderno];
	var kc = kucuns[orderno];
	var kong = kongcs[orderno];
	var q = qts[orderno];
	var shuju = [{"name":"库存","value":kc},{"name":"空仓","value":kong},{"name":"其它","value":q}];
	optionx = {
		    title : {
		    	x:'center',
		        text: qu+"自有仓容统计(单位：吨)"
		    },
		    tooltip : {
		        trigger: 'item',
		        formatter: "{a} <br/>{b} : {c}吨 ({d}%)"
		    },
		    legend: {
		    	orient : 'vertical',
		        y:'center',
		        x:'left',
		        data:leixin,
		    },
		    calculable : true,
		    series : [
		        {
		            name:'数量',
		            type:'pie',
		            radius : '55%',
		            center: ['60%', '50%'],
		            itemStyle:{
	                	  normal:{
	                		  label:{
	                			  show: false 
	                		  },
	                		  labelLine:{
	                			  show: false
	                		  }
	                	  }
	                  },
		            data:shuju
		        }
		    ]
		};
	echarts.init(document.getElementById('containerx')).setOption(optionx);	
}



