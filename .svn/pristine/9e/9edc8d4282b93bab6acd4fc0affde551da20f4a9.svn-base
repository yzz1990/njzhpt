<!DOCTYPE html>
<html><head><meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <title>专职人员信息</title>
    
    <meta name="viewport" content="width=device-width, initial-scale=1.0"> 
    <meta name="description" content="Blue Moon - Responsive Admin Dashboard">
    <meta name="keywords" content="Notifications, Admin, Dashboard, Bootstrap3, Sass, transform, CSS3, HTML5, Web design, UI Design, Responsive Dashboard, Responsive Admin, Admin Theme, Best Admin UI, Bootstrap Theme, Wrapbootstrap, Bootstrap, bootstrap.gallery">
    <meta name="author" content="Bootstrap Gallery">
    
    <!-- FONT AWESOME-->
	<link rel="stylesheet" href="${ctxPath}/css/font-awesome.min.css">

    <%include("/WEB-INF/web/ui/cssjs.html"){}%>
  </head>
<%
	var obj={},url=ctxPath+"/cangchugl/addZzbg",title="新增专职人员信息";
	if (isNotEmpty(parameter.id)) {
		var zzbg = @com.zkzy.njzhpt.dao.CangchuglDao.findZzbg({"id":parameter.id,"page":"1","rows":"100"});
		obj = zzbg.list[0];
		url = ctxPath+"/cangchugl/upZzbg";
		title = "编辑专职人员信息";
	};
	
%>
<!-- var obj=@com.zkzy.njzhpt.dao.CangchuglDao.findanquantj({"id":parameter.id,page:1,rows:1}).getList().get(0);	 -->
  <body>

        <div class="dashboard-wrapper-lg" style="padding: 0">
            <div class="row">
              <div class="col-lg-12 col-md-12">
                <div class="widget">
                  <div class="widget-header">
                    <div class="title">
                        	专职人员信息                                        
                    </div>
                  </div>
                  <div class="widget-body">
				       <form id="contentform" role="form"  action="${url}" method="post" style="overflow: hidden;">
					       <input type="hidden" name="id" value="${obj.id}">
							<div class="row">
								<div class="col-xs-6">	
									<div class="form-group">
										<label for="qyzzjgdm">企业组织机构代码:</label>
										<input id="qyzzjgdm" name="qyzzjgdm" class="form-control" placeholder="企业组织机构代码" value="${obj.qyzzjgdm}">
									</div>	
								</div>
								<div class="col-xs-6">	
									<div class="form-group">
										<label for="quyu">所属辖区:</label>
										<input id="quyu" name="quyu" class="form-control" placeholder="所属辖区" value="${obj.quyu}">
									</div>	
								</div>
								
							</div>
							<div class="row">
								<div class="col-xs-6">	
									<div class="form-group">
										<label for="xm">姓名:</label>
										<input id="xm" name="xm" class="form-control" placeholder="姓名" value="${obj.xm}">
									</div>	
								</div>
								<div class="col-xs-6">	
									<div class="form-group">
										<label for="xl">学历:</label>
										<input id="xl" name="xl" class="form-control" placeholder="学历" value="${obj.xl}">
									</div>	
								</div>
								
							</div>
							<div class="row">
								<div class="col-xs-6">
									<div class="form-group">
										<label for="zy">专业:</label>
										<input id="zy" name="zy" class="form-control" placeholder="专业" value="${obj.zy}">
									</div>
								</div>
								<div class="col-xs-6">	
									<div class="form-group">
										<label for="jszc">技术职称:</label>
										<input id="jszc" name="jszc" class="form-control" placeholder="技术职称" value="${obj.jszc}">
									</div>	
								</div>
							</div>
							<div class="row">
								<div class="col-xs-6">
									<div class="form-group">
										<label for="zgzsmc">资格证书名称:</label>
										<input id="zgzsmc" name="zgzsmc" class="form-control" placeholder="资格证书名称" value="${obj.zgzsmc}">
									</div>
								</div>
								<div class="col-xs-6">	
									<div class="form-group">
										<label for="fzdw">发证单位:</label>
										<input id="fzdw" name="fzdw" class="form-control" placeholder="发证单位" value="${obj.fzdw}">
									</div>	
								</div>
							</div>
							<div class="row">
								<div class="col-xs-12">	
									<div class="form-group">
										<label for="rzsj">入职时间:</label>
										<input id="rzsj" name="rzsj" class="form-control" placeholder="建设时间" value="${obj.rzsj}">
									</div>	
								</div>
							</div>
					</form>
					<!-- 	<input type="hidden" id="btn1-s" ></input> -->
                  </div>
                </div>
	
                
              </div>
            </div>
            <!-- Row End -->
        </div>

<script type="text/javascript">

function savezzbg() {
	var form = $(".widget-body form");
	$.ajax({
		url: form.attr("action"),
		dataType: "json",
		data: form.serialize(),
		success: function(msg) {
			if (!msg.ret) {
				alert(msg.message);
			} else {
				alert("保存成功");
			}
		}
	});
}


