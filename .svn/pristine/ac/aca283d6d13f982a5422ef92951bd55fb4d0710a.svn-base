<script type="text/javascript">
var datatable;
$(function(){
	selectKDMC();
	$("#liuchenghao,#ccku,#ccdian,#qyzzjgdm,#pz,#cfbm").select2();
	$.ajax({
		 url:'${ctxPath}/lunhuan/findlcbylc',
		 type:'post',
		 data:{},
		 dataType:'json',
		 success:function(data){
			$.each(data.list,function(i, n){
					$("#liuchenghao").append("<option value="+n.liuchenghao+">"+n.name+"</option>");
			});
		 }
	});
	$('.file_upload').uploadifive({
		'auto' : true,   //自动上传 
        'uploadScript' : '${ctxPath}/excel/uploadlhsqxwFile',  //处理上传文件Action路径
        'buttonClass' : 'file_upload',
        'fileObjName' : 'file',        //文件对象
  	    'buttonText'   : '上传行文',   //按钮显示文字 
  	 	'queueID'      : 'tip-queue', //提示信息放置目标 
//  		'fileTypeExts'     : '*.xlsx;*.xls;*.png',   //允许上传文件后缀名
		'formData': { 'id': $("#biaoid").val() },
  	    'onUploadComplete' : function(file, data) { //文件上传成功后执行
	    	var obj = JSON.parse(data);
	    	$("#scfilename").html(obj.scfilename);
	    	$("#sqxingwen").val(obj.scfilename);
	    	$("#scfilename").attr("href","${ctxPath}"+obj.filepath);
	    	$("#scfilename").attr("download",obj.scfilename);
    	}
	});
	
});

var zongkuc;
//根据选择的仓查询生产年限和品种
function selectYearPZ(){
	var cfbm=$("#cfbm").val();
	var kdbm=$("#ccdian").val();
	var qyzzjgdm=$("#ccku").val();
	 $("#scnx").val("");
	 $("#pz").html("");
	 var pinzhong = "<option value=''>请选择品种</option>";
	 $("#pz").append(pinzhong);
	 $("#lhsl").val("");
	 $("#lch_xjzb").val("");
	 $("#lch_xmzb").val("");
	 $("#lch_jdzb").val("");
	 $("#lch_xdzb").val("");
			$.ajax({
				url:'${ctxPath}/lunhuan/findYearPZ.do',
				type:'post',
				data:{"qyzzjgdm":qyzzjgdm,"kdbm":kdbm,"cfbm":cfbm},
				dataType:'json',
				success:function(data){
						
						 if(null!=data.ret){
							 $("#scnx").val(data.ret.year);
							 $("#pz").html("");
							 var pinzhong = "<option value="+data.ret.vCode+">"+data.ret.vName+"</option>";
							 $("#pz").append(pinzhong);
							 zongkuc=data.ret.zongkucun;
							 $("#lhsl").val(data.ret.zongkucun);
						 lchzb();
						 }
				},
				error:function(){
					alert("error");
				}
				
				
			});
}



function Change(){
	$("#qymc").html("");
	var xian = $("#szqy option:selected").text().trim();
	var op = '<option value="">请选择企业名称</option>';
    $("#qymc").append(op);
    var kd = '<option value="">请选择库点名称</option>';
    $("#lhsq_kdmc").append(kd);
	$.ajax({
		url:'${ctxPath}/lunhuan/findcompany',
		type:'post',
		data:{"xian":xian},
		dataType:'json',
		success:function(data){
			for(var i = 0; i < data.length; i++){
				 var op = "<option value="+data[i].qyzzjgdm+">"+data[i].qymc+"</option>";
			     $("#qymc").append(op);
			}
		},
		error:function(){
			alert("error");
		}
		
		
	});
	
	
}
	function shangchuan(){
		
		if($("#liuchenghao").val()==""){
			alert("请选择流程号 ");
			return false;
		}else if($("#qyzzjgdm").val()==""){
			alert("请选择承储企业 ");
			return false;
		}else if($("#ccku").val()==""){
			alert("请选择承储库 ");
			return false;
		}else if($("#ccdian").val()==""){
			alert("请选择承储点 ");
			return false;
