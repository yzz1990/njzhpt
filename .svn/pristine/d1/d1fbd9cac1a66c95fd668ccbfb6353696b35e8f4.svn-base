<script type="text/javascript">
$('#qrtz_lrsj').datetimepicker({
	minView: "month",
	format:'yyyy-mm-dd',
	language: 'zh-CN',
	autoclose: 1,
	startView: 2,
});
$(function(){
	$('.file_upload').uploadifive({
		'auto' : true,   //自动上传 
        'uploadScript' : '${ctxPath}/excel/uploadqrtzxwFile',  //处理上传文件Action路径 
        'buttonClass' : 'file_upload',
        'fileObjName' : 'file',        //文件对象
  	    'buttonText'   : '上传行文',   //按钮显示文字 
  	 	'queueID'      : 'tip-queue', //提示信息放置目标 
//  		'fileTypeExts'     : '*.xlsx;*.xls;*.png',   //允许上传文件后缀名
		'formData': { 'id': $("#biaoid").val() },
  	    'onUploadComplete' : function(file, data) { //文件上传成功后执行
	    	var obj = JSON.parse(data);
	    	$("#scfilename").html(obj.scfilename);
	    	$("#qrtz_xw").val(obj.scfilename);
	    	$("#scfilename").attr("href","${ctxPath}"+obj.filepath);
	    	$("#scfilename").attr("download",obj.scfilename);
    	}
	});

	
});

$(document).ready(function() {
	  $(".js-example-basic-single").select2();
});

function quxiao(){
	parent.dialog4.close();
}
	
	function pizhun(){
		if($("#qrtz_qrwh").val()==""){
			alert("请输入确认文号！");
			return false;
		}else if($("#qrtz_lrsj").val()==""){
			alert("请选择轮入时间!");
			return false;
		}else{
			$.ajax({
   				url: "${ctxPath}/lunhuan/saveLunHuanQRTZ.do",
   				dataType: "json",
   				data:$("#sc").serialize(),
   				success: function(msg) {
   					if (msg.ret) {
   						alert("确认通知成功");
   						parent.window.dialog4.close();
   						parent.window.datatable.ajax.reload(null,true);
   					}else{
   						alert("确认通知失败");
   					}
   				}
   			});
		}
	   		 
		
	}
	</script>
