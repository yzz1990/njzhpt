<script type="text/javascript">


Array.prototype.S=String.fromCharCode(2);
Array.prototype.in_array=function(e){
    var r=new RegExp(this.S+e+this.S);
    return (r.test(this.S+this.join(this.S)+this.S));
};

var mydate = new Date();
var tyear = mydate.getFullYear();
var tday = mydate.toLocaleDateString();

var ccku = "";
var cckuname = "";
var ccdian = "";
var pz = "";

$(function(){
	
	//加载轮入数量
	var q = "&sqwh=${sqwh}";
	document.querySelector('#cftable').dataset.queryparam=q;
	 datatable = $('#cftable').DataTable({
		 "processing": true,
	    "serverSide": true,
	    "paging": false,
		"info": false,
        "columns": [
            {"data":"cfbm","title":"仓号"},
            {"data":"cflhsl","title":"轮入数量"}
	 	]
	}); 
	 //加载轮出审批和数量
	 if("${obj.lrlx}"=="0"){		//普通轮入
		 ccku = "${obj.cckuname}";
		 ccdian = "${obj.ccdian}";
		 pz = "${obj.pz}";
	 }else if("${obj.lrlx}"=="1"){	//易库点轮入
		 cckuname="${obj.bgkd}";
		 pz = "${obj.pz}";
	 }else if("${obj.lrlx}"=="2"){	//易品种轮入
		 ccku = "${obj.cckuname}";
		 ccdian = "${obj.ccdian}";
		 pz = "${obj.pz}";
	 }
	 $.ajax({
		 url:'${ctxPath}/lunhuan/findlunchu.do',
		 type:'post',
		 data:{"ccku":ccku,"ccdian":ccdian,"pz":pz,"cckuname":cckuname},
		 dataType:'json',
		 success:function(data){
			 
		 },
		 error:function(){
			 alert("获取轮出审批失败");
		 }
	 })
	 

});


</script>
