			url:'${ctxPath}/renyuan/findxunzhendx',
			type:'post',
			data:{"page":1,"rows":1000,"beianbianhao":"${obj.beianbianhao}"},
			dataType:'json',
			success: function(data) {
				var qyzzjgdm=$("#qiyeid option:selected").attr("qydm");
				 var kdbm=$("#kudian option:selected").val();
				
				 $.each(data.list,function(i, n){
					 if(data.list.length-1!=i){
						 indexcf++;
