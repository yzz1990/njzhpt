<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">

<title>增加人员</title>

	<meta name="viewport" content="width=device-width, initial-scale=1.0"> 
    <meta name="description" content="Blue Moon - Responsive Admin Dashboard">
    <meta name="keywords" content="Notifications, Admin, Dashboard, Bootstrap3, Sass, transform, CSS3, HTML5, Web design, UI Design, Responsive Dashboard, Responsive Admin, Admin Theme, Best Admin UI, Bootstrap Theme, Wrapbootstrap, Bootstrap, bootstrap.gallery">
    <meta name="author" content="Bootstrap Gallery">
    
    <link rel="stylesheet" href="${ctxPath}/css/font-awesome.min.css">
 	<script src="${ctxPath}/js/jquery.min.js"></script>

    <%include("/WEB-INF/web/ui/cssjs.html"){}%>

</head>
<%
	var obj={},url=ctxPath+"/emergency/addrenyuanyj";
	var fobj={};
	fobj = @com.zkzy.njzhpt.dao.EmergencyDAO.querytreeyj({"id":parameter.fid,page:1,rows:1}).getList().get(0);
	var fid = parameter.fid;
	var cengshu = parameter.cengshu;
	if (isNotEmpty(parameter.id)) {
		fobj=@com.zkzy.njzhpt.dao.EmergencyDAO.querytreeyj({"id":parameter.id,page:1,rows:1}).getList().get(0);
		obj=@com.zkzy.njzhpt.dao.EmergencyDAO.querytreeyj({"id":parameter.id,page:1,rows:1}).getList().get(0);
		url = ctxPath+"/emergency/upbumen";
	};
%>
<script type="text/javascript">

$(window).load(function() { 
	var cs = parseInt("${cengshu}") + 1;
	$("#cengshu").val(cs);
	$("#isend").val(0);
	$("#leixing").val("yj");
	$("#shangji").val("${fid}");
});

</script>


<body>
	 <div class="dashboard-wrapper-lg" >
	 	<form id="contentform" role="form"  action="${url}"  method="post" style="overflow: hidden;">
	 		<input type="hidden" id="id" name="id" class="form-control" placeholder="id" value="${obj.id}">
	 		<input type="hidden" id="cengshu" name="cengshu" class="form-control" placeholder="chengshu" value="${obj.cengshu}">
	 		<input type="hidden" id="isend"  name="isend" class="form-control" placeholder="isend" value="${obj.isend}">
	 		<input type="hidden" id="leixing"  name="leixing" class="form-control" placeholder="leixing" value="${obj.leixing}">
	 		<input type="hidden" id="shangji"  name="shangji" class="form-control" placeholder="shangji" value="${obj.shangji}">
            <div class="row">
              <div class="col-lg-12 col-md-12">
                <div class="widget">
                  <div class="widget-header">
                    <div class="title">
                        	基本信息                                  
                    </div>
                  </div>
                  <div class="widget-body">
						<div class="row">
							<div class="col-xs-12">	
								<div class="form-group">
									<label for="name">*姓名:</label>
									<input id="name" name="name" class="form-control" placeholder="姓名" value="${obj.name}">	
								</div>	
							</div>
							<div class="col-xs-12">	
								<div class="form-group">
									<label for="zhiwu">职务:</label>
									<input id="zhiwu" name="zhiwu" class="form-control" placeholder="职务" value="${obj.zhiwu}">	
								</div>	
							</div>
							<div class="col-xs-12">	
								<div class="form-group">
									<label for="bumen">部门:</label>
									<input id="bumen" name="bumen" class="form-control" placeholder="部门" readonly="readonly"  value="${fobj.name}">	
								</div>	
							</div>
							<div class="col-xs-12">	
								<div class="form-group">
									<label for="diqu">地区:</label>
									<input id="diqu" name="diqu" class="form-control" placeholder="地区" readonly="readonly"  value="${fobj.diqu}">	
								</div>	
							</div>
							<div class="col-xs-12">	
								<div class="form-group">
									<label for="">*排序:</label>
									<input id="orderno" name="orderno" class="form-control" placeholder="排序" value="${fobj.orderno}">	
								</div>	
							</div>
							<div class="col-xs-12">	
								<div class="form-group">
									<label for="guding">固定电话:</label>
									<input id="guding" name="guding" class="form-control" placeholder="固定电话" value="${obj.guding}" 
									onKeyPress="if (event.keyCode!=46 && event.keyCode!=45 && (event.keyCode<48 || event.keyCode>57)) event.returnValue=false">	
								</div>	
							</div>
							<div class="col-xs-12">	
								<div class="form-group">
									<label for="tel">移动电话:</label>
									<input id="tel" name="tel" class="form-control" placeholder="移动电话" value="${obj.tel}" 
									onKeyPress="if (event.keyCode!=46 && event.keyCode!=45 && (event.keyCode<48 || event.keyCode>57)) event.returnValue=false">	
								</div>	
							</div>
							<div class="col-xs-12">	
								<div class="form-group">
									<label for="beizhu">备注:</label>
									<input id="beizhu" name="beizhu" class="form-control" placeholder="备注" value="${obj.beizhu}">	
								</div>	
							</div>
						</div>
					</div>
				</div>
			</div>
		</div>
		
	</form>
	</div>
            <!-- Row End -->
            
    <script type="text/javascript">
    (function ($) {
        $.getUrlParam = function (name) {
            var reg = new RegExp("(^|&)" + name + "=([^&]*)(&|$)");
            var r = window.location.search.substr(1).match(reg);
            if (r != null) return unescape(r[2]); return null;
        }
    })(jQuery);

    function saverenyuan() {
    	var name = $("#name").val();
    	var orderno = $("#orderno").val();
    	
    	var tishi = "";
    	
    	if(name == "" || name == null){
    		tishi += "姓名不能为空 ";
    	}
    	if(orderno == "" || orderno == null){
    		tishi += "排序不能为空";
    	}
    	
    	if(tishi != ""){
    		alert(tishi);
    	}else{
    		var form = $("#contentform");
        	$.ajax({
        		url: form.attr("action"),
        		type:"post",
        		dataType:"json",
        		data: form.serialize(),
        		success: function(msg) {
        			if (!msg.ret) {
        				alert(msg.message);
        			} else {
        				alert("保存成功");
        				 parent.window.dialog1.close();
			        	  parent.window.datatable.ajax.reload(null,true);
        			}
        			window.parent.gettree();
        		},
        		error:function(){
        			alert("保存失败,请检查内容是否重复");
        		}
        	});
    	}
    	
    }
    
    </script>
            
</body>


</html>