<!DOCTYPE html>
<html><head><meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <title>模块信息</title>
    
    <meta name="viewport" content="width=device-width, initial-scale=1.0"> 
    <meta name="description" content="Blue Moon - Responsive Admin Dashboard">
    <meta name="keywords" content="Notifications, Admin, Dashboard, Bootstrap3, Sass, transform, CSS3, HTML5, Web design, UI Design, Responsive Dashboard, Responsive Admin, Admin Theme, Best Admin UI, Bootstrap Theme, Wrapbootstrap, Bootstrap, bootstrap.gallery">
    <meta name="author" content="Bootstrap Gallery">
    
    <!-- FONT AWESOME-->
	<link rel="stylesheet" href="${ctxPath}/css/font-awesome.min.css">

    <%include("/WEB-INF/web/ui/cssjs.html"){}%>
  </head>
<%
	var obj={},url=ctxPath+"/cangchugl/addCaidanweihu",title="新增模块信息";
	if (isNotEmpty(parameter.id)) {
		var spgl = @com.zkzy.njzhpt.dao.CangchuglDao.findcaidanfangfaweihu({"id":parameter.id,"page":"1","rows":"100"});
		obj = spgl.list[0];
		url = ctxPath+"/cangchugl/upCaidanweihu";
		title = "编辑模块信息";
	};
	
%>
<!-- var obj=@com.zkzy.njzhpt.dao.CangchuglDao.findanquantj({"id":parameter.id,page:1,rows:1}).getList().get(0);	 -->
  <body>

        <div class="dashboard-wrapper-lg" style="padding: 0">
            <div class="row">
              <div class="col-lg-12 col-md-12">
                <div class="widget">
                  <div class="widget-header">
                    <div class="title">
                        	模块信息                                        
                    </div>
                  </div>
                  <div class="widget-body">
				       <form id="contentform" role="form"  action="${url}" method="post" style="overflow: hidden;">
					       <input type="hidden" name="id" value="${obj.id}">
							<div class="row">
								<div class="col-xs-6">	
									<div class="form-group">
										<label for="code">排序代码:</label>
										<input id="code" name="code" class="form-control" placeholder="排序" value="${obj.code}"> 
									</div>	
								</div>
								<div class="col-xs-6">	
									<div class="form-group">
										<label for="name">菜单名称:</label>
										<input id="name" name="name" class="form-control" placeholder="菜单名称" value="${obj.name}"> 									</div>	
								</div>
							</div>
							<div class="row">
								<div class="col-xs-6">	
									<div class="form-group">
										<label for="jiequname">截取字段:</label>
										<input id="jiequname" name="jiequname" class="form-control" placeholder="截取字段" value="${obj.jiequname}">
									</div>	
								</div>
								<div class="col-xs-6">	
									<div class="form-group">
										<label for="mokuai">模块名称:</label>
										<input id="mokuai" name="mokuai" class="form-control" placeholder="模块名称" value="${obj.mokuai}">
									</div>	
								</div>
							</div>
							<div class="row">
								<div class="col-xs-6">	
									<div class="form-group">
										<label for="isedit">类型:</label>
										<input id="isedit" name="isedit" class="form-control" placeholder="类型" value="${obj.isedit}">
									</div>	
								</div>
							</div>
					</form>
					<!-- 	<input type="hidden" id="btn1-s" ></input> -->
                  </div>
                </div>
	
                
              </div>
            </div>
            <!-- Row End -->
        </div>

<script type="text/javascript">

function saveykgl() {
	
	var form = $(".widget-body form");
	$.ajax({
		url: form.attr("action"),
		dataType: "json",
		data: form.serialize(),
		success: function(msg) {
			if (!msg.ret) {
				alert(msg.message);
			} else {
				alert("保存成功");
			}
		}
	});
}



</script>

</body>
</html>