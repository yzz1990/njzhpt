function saveCangfang(){
	var form = $(".dashboard-wrapper-lg form");
	$.ajax({
		url:form.attr("action"),
		data:$("form").serialize(),
		dataType:"JSON",
		success:function(data){
			if(data.ret){
				alert('保存成功');
				var ID=$("#id").val();
				if(ID!=""){						
						window.parent.frames["ab"].window.frames["if4"].contentWindow.edit();
				}else{					
						window.parent.frames["ab"].window.frames["if4"].contentWindow.edit();
				}
			}else{
				alert(data.message);
			}
		}
	});
}


