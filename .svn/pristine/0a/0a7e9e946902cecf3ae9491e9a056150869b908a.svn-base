	for(var i = 0; i < 3 ; i++){
		var op;
		if(i == 0){
			op = '<option selected value="'+(year-i)+'">'+(year-i)+'</option>';
		}else{
			op = '<option value="'+(year-i)+'">'+(year-i)+'</option>';
		}
		$("#niandu").append(op);
	}
	
