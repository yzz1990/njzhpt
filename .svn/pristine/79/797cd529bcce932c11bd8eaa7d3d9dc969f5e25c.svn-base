 layout("/WEB-INF/web/ui/globalStyleDialog.html"){
%>
	<!----------- shiping ------------>
	<script src="${ctxPath}/js/preview.js?version=3.0.4.150415"></script>
	<!----------- shiping ------------>
	<link rel="stylesheet" href="${ctxPath}/css/bootstrap.css" >
	
	 <style type="text/css">
