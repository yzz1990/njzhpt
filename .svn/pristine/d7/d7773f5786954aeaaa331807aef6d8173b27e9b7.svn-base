	.divclick{
		border:2px solid #0000FF;
	}
	.sp{
		border:1px solid #B0C4DE;
		z-index:1;
		background:#4D0000;
	}
	.xp{
		background-color:green;
		z-index:9999;
	} 
	.ss{
		height:100%;

