										<select id="cflxbh"  name="cflxbh" class="form-control"  onchange="changeCflxbh()" >
										</select>
									</div>
								</div>
								<div class="col-xs-4">
									<div class="form-group">
										<label for="zyywmc">主要业务:</label>										
										<select id="zyywbh"  name="zyywbh" class="form-control"  onchange="changeZyywbh()" >
										</select>
									</div>
								</div>
							</div> 
							
							<div class="row">
								<div class="col-xs-4">	
									<div class="form-group">
										<label for="sjcr"><font color="red" style="font-size:large;">*</font>设计仓容(吨):</label>
										<input id="sjcr" name="sjcr" class="form-control" placeholder="设计仓容(吨)"  value="${obj.sjcr}"
										onKeyPress="if (event.keyCode!=46 && event.keyCode!=45&&(event.keyCode<48 || event.keyCode>57)) event.returnValue=false">
									</div>
									<script>
										$(function(){
											var sjcr="${obj.sjcr}";
											if(sjcr!=null&&sjcr!=""){
												$("#sjcr").val(parseFloat(sjcr).toFixed(3));
											}
											var shjcr="${obj.shjcr}";
											if(shjcr!=null&&shjcr!=""){
												$("#shjcr").val(parseFloat(shjcr).toFixed(3));
											}
											var cncd="${obj.cncd}";
											if(cncd!=null&&cncd!=""){
												$("#cncd").val(parseFloat(cncd).toFixed(2));
											}
											var cnkd="${obj.cnkd}";
											if(cnkd!=null&&cnkd!=""){
												$("#cnkd").val(parseFloat(cnkd).toFixed(2));
											}
											var ykgd="${obj.ykgd}";
											if(ykgd!=null&&ykgd!=""){
												$("#ykgd").val(parseFloat(ykgd).toFixed(2));
											}
											var cnzj="${obj.cnzj}";
											if(cnzj!=null&&cnzj!=""){
												$("#cnzj").val(parseFloat(cnzj).toFixed(2));
											}
										})
									</script>	
								</div>
								<div class="col-xs-4">
									<div class="form-group">
										<label for="shjcr"><font color="red" style="font-size:large;">*</font>实际仓容(吨):</label>
										<input id="shjcr" name="shjcr" class="form-control" placeholder="实际仓容(吨)" value="${obj.shjcr}"
										onKeyPress="if (event.keyCode!=46 && event.keyCode!=45&&(event.keyCode<48 || event.keyCode>57)) event.returnValue=false">
									</div>
								</div>
								<div class="col-xs-4">
									<div class="form-group">
										<label for="jsnd">建设年代:</label>
										<input id="jsnd" name="jsnd" class="form-control" placeholder="建设年代" value="${obj.jsnd}" onkeyup="value=value.replace(/[^\d]/g,'') " onbeforepaste="clipboardData.setData('text',clipboardData.getData('text').replace(/[^\d]/g,''))">
									</div>
								</div>
							</div>
                  	</div>
                </div>
                
                <div class="widget">
                  <div class="widget-header">
                    <div class="title">
                        	仓房基本信息                                        
                    </div>
                  </div>
                  <div class="widget-body">
                  
							<div class="row">
								<div class="col-xs-3">	
