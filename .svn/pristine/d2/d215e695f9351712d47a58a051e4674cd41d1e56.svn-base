<%
	layout(gisCityHtml){
%>
<style>
	.container-fluid {
	     padding-right: 0px; 
	     padding-left: 0px; 
	     margin-right: 0px; 
	     margin-left: 0px; 
	}
	.panel.panel-default {
    	border-top-width: 3px;
   	 	margin-top: 0px; 
	}	
</style>
<script>
$(function(){
	$("#fxlykdxx").attr("height",window.parent.$(".aside-inner").height());
	if("${session.userinfo.role.code}"=="systemuser"||"${session.userinfo.role.code}"=="njslsj"||"${session.userinfo.role.code}"=="jdjc"||"${session.userinfo.role.code}"=="jldjkscz"||"${session.userinfo.role.code}"=="cbtk"||"${session.userinfo.role.code}"=="jzhcky"){
		$("#fxlykdxx").attr("src","${gisurl}indexLYDian.jsp?role=${session.userinfo.role.code}&code=''");
	 }else if("${session.userinfo.role.code}"=="qxlsj"){
		 $.ajax({
			 url:'${ctxPath}/shouchukeshihua/findquxian',
			 type:'post',
			 data:{"name":"${session.userinfo.area.name}"},
			 dataType:'json',
			 success:function(data){					 
				 $("#fxlykdxx").attr("src","${gisurl}indexLYDian.jsp?role=${session.userinfo.role.code}&code="+data[0].xzqhdm);				 
			 } 
		 });
	 }else if("${session.userinfo.role.code}"=="qiye"){
		 
		 $.ajax({
			 url:'${ctxPath}/jibenxinxi/findQiyexinxi',
			 type:'post',
			 dataType:'json',
			 success:function(data){
				 //alert(data.list[0].qyzzjgdm);
	     		$("#fxlykdxx").attr("src","${gisurl}indexLYDian.jsp?role=${session.userinfo.role.code}&code="+data.list[0].qyzzjgdm);			 
			 }
				 
		 });
	 }
	
});
</script>
<div class="panel panel-default">	
	<div class="panel-body" >
		<div class="container-fluid">			
			<iframe id="fxlykdxx" src="" width="98%" height="635px;" style="position: absolute;margin-top: 0px;" frameborder="no" border="0" marginwidth="0" marginheight="0" scrolling="no" allowtransparency="yes"></iframe>
		</div>
	</div>
</div>
<%
	}
%>