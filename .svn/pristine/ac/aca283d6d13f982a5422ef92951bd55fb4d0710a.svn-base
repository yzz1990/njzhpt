function yulan(){
	if(""==$("#lhsl").val()||null==$("#lhsl").val()){
		alert("请先完善轮换申请信息！");
		return false;
	}
	var pzbm=$("#pz").val();
	var data={};
	data.qymc=$("#qymc").val();  //企业名称
	data.pz=$("#pz").text();	 //品种名称
	data.ccjh_xj=$("#ccjh_xj").val();  //市级储备规模量
	if(pzbm.substring(0,3) == '111'){
		data.fpzccjh=$("#ccjh_xm").val();		//分品种承储规模量
		data.fpzswkc=$("#swkc_xm").val();		//分品种库存量
		data.fpzzb=$("#xmzb").val();			//分品种库存比例
		data.lch_fpzzb=$("#lch_xmzb").val();			//轮出后分品种库存比例
		data.ndfpzkclist=$("#xmfndkc").val();	//分品种分年度库存
	}else if(pzbm.substring(0,4) == '1132'){
		data.fpzccjh=$("#ccjh_jd").val();
		data.fpzswkc=$("#swkc_jd").val();
		data.fpzzb=$("#jdzb").val();
		data.lch_fpzzb=$("#lch_jdzb").val();
		data.ndfpzkclist=$("#jdfndkc").val();
	}else if(pzbm.substring(0,4) == '1131'){
		data.fpzccjh=$("#ccjh_xd").val();
		data.fpzswkc=$("#swkc_xd").val();
		data.fpzzb=$("#xdzb").val();
		data.lch_fpzzb=$("#lch_xdzb").val();
		data.ndfpzkclist=$("#xdfndkc").val();
	}
	var myDate = new Date();
	var monthpast = myDate.getMonth() + 1;
	var day = myDate.getDate();  
	var year=myDate.getFullYear();
	
	myDate.setMonth(myDate.getMonth()+parseInt(4));
	var year1=myDate.getFullYear();
	var month1=(myDate.getMonth()+1);
	
	data.year=year;
	data.day=day;
	data.lr_year=year1;
	data.lr_month=month1;
	data.month=monthpast;				//月末
	data.swkc_xj=$("#swkc_xj").val();
	data.xjzb=$("#xjzb").val();   //月末市级储备库存总量 
	
	data.niandu=$("#scnx").val();		//生产年度
	data.ccdian=$("#ccdian option:selected").text();			//库点名称
	data.cfmc=$("#cfbm option:selected").text();			//仓房名称
