<script type="text/javascript">
var xian;
var dep;
var date=new Date;
var year = parseInt(date.getFullYear());
year = year + 2;
$(window).load(function() { 
	xian = "${session.userinfo.area.name}";
	dep = "${session.userinfo.role.code}";
	var objyear = parseInt("${obj.niandu}");
	$.ajax({
		url:'${ctxPath}/shouchukeshihua/findquxian',
		type:'post',
		dataType:'json',
		success:function(data){
			$.each(data,function(i, n){
				if(n.name==xian || n.name == "${obj.diqu}"){
					$("#diqu").append("<option value="+n.name+"  selected>"+n.name+"</option>");
				}else{
					$("#diqu").append("<option value="+n.name+">"+n.name+"</option>");
				}
			});
			if(dep == "qxlsj"){
				$("#diqu").attr("disabled",true);
			}
		},
		error:function(){
			alert("加载区县失败");
		}
	});
	for(var i = 0; i < 7; i++){
		if((year - i) == objyear){
			$("#niandu").append("<option value='"+(year - i)+"' selected>"+(year - i)+"</option>");	
		}else{
			$("#niandu").append("<option value='"+(year - i)+"'>"+(year - i)+"</option>");	
		}
	}
	
});



</script>



