	$("#ccdian").append(op);
	/* var cc = '<option value="">请选择仓廒</option>';
	$("#cfbm").append(cc); */
	
	$.ajax({
		url : '${ctxPath}/chubeiliangJH/chengchudian',
		type : 'post',
		data : {
			"qyzzjgdm" : qyzzjgdm,
		},
		dataType : 'json',
		success : function(data) {
			 for (var i = 0; i < data.length; i++) {
				 if("${obj.ccku}"==data[i].qyzzjgdm&&"${obj.ccdian}"==data[i].kdbm){
					 $("#ccdian").append("<option value="+data[i].kdbm+" selected>" + data[i].kdmc+ "</option>");
				 }else{
					$("#ccdian").append("<option value="+data[i].kdbm+">" + data[i].kdmc+ "</option>");
				 }
			} 
		
			/* $('#kdbm').select2({
			  data:data

			}); */
		}
