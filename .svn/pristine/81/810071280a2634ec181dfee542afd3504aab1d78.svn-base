                	var chakan= '<button type="button" class="btn btn-success btn-xs" onclick="lunhuanpizhun1(\''+row["id"]+'\')"><span style="color:white">查看</span> </button>';
      				 if(parseInt(row["lcstatus"].substring(0,1))>=(parseInt(row["jiedian"])+1)){
