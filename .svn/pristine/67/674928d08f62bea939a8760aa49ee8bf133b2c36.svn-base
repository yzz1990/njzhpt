function getKudian(xzqhdm){
	$.post("${ctxPath}/cangchugl/findkudian",{xzqhdm:xzqhdm},function(data){
		$.each(data,function(i, n){
			var quyubm = n.xzqhdm;
			var qyzzjgdm = n.qyzzjgdm;
			var name = n.kdmc;
			var ID = n.ID;
		    var kdbm=n.kdbm;
			
			if(name=="灵山库区"||name=="南京石埠桥粮食储备库有限公司"||name=="将军山-大库"||name=="四合库"||name=="曹家桥库"||name=="罗古墩"||name=="六郎-大库"||name=="清修"||name=="漕塘储备库"||name=="顾陇储备库"||name=="朱家边库"){				
			 $("#"+xzqhdm).append("<ul><li class='parent_li' id='"+ID+"' style='display:none;'><span onclick='showkd(\""+ID+"\")'><img alt='' src='${ctxPath}/resource/img/alarm-in.png' style='margin-right: 4px;margin-top: -6px;'>"+name+"</span></li></ul>");
			
			}else{				
			 $("#"+xzqhdm).append("<ul><li class='parent_li' id='"+ID+"' style='display:none;'><span onclick='showkd(\""+ID+"\")'><img alt='' src='${ctxPath}/resource/img/alarm-in-dis.png' style='margin-right: 4px;margin-top: -6px;'>"+name+"</span></li></ul>");
			}
			 getSp(quyubm,qyzzjgdm,ID,kdbm);
		});
	},'json');
}

