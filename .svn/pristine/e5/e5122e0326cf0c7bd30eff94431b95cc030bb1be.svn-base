<script>

var xians = [];
var kucuns = [];
var kongcs = [];
var qts = [];

$(function () {	
	var ecConfig = require('echarts/config');
	$.ajax({
		url:'${ctxPath}/zonghe/findcflyl',
		type:'post',
		dataType:'json',
		success:function(data){
			  var strhtml=""; 
		    	  for(var j=0;j<data.crlist.length;j++){		    		  
		    	    	strhtml+="<tr>"
		    	    for(var h=0;h<data.crlist[j].length;h++){	    	
		    	  		strhtml+="<td rowspan='1' style='text-align:center;border:1px solid #7CBDF7'>"+data.crlist[j][h]+"</td>"
		    	    }
		         		strhtml+="</tr>";
		    	  }
	    	     $("#zcr").append(strhtml);
		      
	    	     option = {
	    	    		    title: {
	    	    		        text: '全市仓容统计(单位:吨)',
	    	    		        x:"center"
	    	    		       },
	    	    		    tooltip: {
	    	    		        trigger: 'axis',
	    	    		        formatter: "{b} <br/>{a0} : {c0}吨<br/>{a1} : {c1}吨",
	    	    		        axisPointer: {
	    	    		            type: 'shadow'
	    	    		        }
	    	    		    },
	    	    		    legend: {
	    	    		        x: 'bottom',
	    	    		        data:['总仓容','自有仓容']
	    	    		    },
	    	    		    toolbox: {
	    	    		        show: true,
	    	    		        orient: 'vertical',
	    	    		        y: 'center'
	    	    		    },
	    	    		    calculable: true,
	    	    		    grid: {
	    	    		        y: 80,
	    	    		        y2: 40,
	    	    		        x2: 40
	    	    		    },
	    	    		    xAxis: [
	    	    		        {
	    	    		            type: 'category',
	    	    		            data: ['市区','江宁区','浦口区','六合区','溧水区','高淳区']
	    	    		        }
	    	    		    ],
	    	    		    yAxis: [
	    	    		        {
	    	    		            type: 'value'
	    	    		        }
	    	    		    ],
	    	    		    series: [
	    	    		        {
	    	    		            name: '总仓容',
	    	    		            type: 'bar',
	    	    		            data: data.crlist1
	    	    		        },
	    	    		        {
	    	    		            name: '自有仓容',
	    	    		            type: 'bar',
	    	    		            data: data.crlist2
	    	    		        }
	    	    		    ]
	    	    		};
	    	     echarts.init(document.getElementById('containerxzcr')).setOption(option);	
	    	     
			}
		});
	
	//仓房类型
	
	var cflxmc = ["平房仓","浅圆仓","立筒仓","楼房仓","储粮罩棚","仓间罩棚","铁路罩棚","其他简易仓","其他"];
	var qscflx = [0,0,0,0,0,0,0,0,0];
	
	datatable = $('#cflx').DataTable({
		"processing": true,
	    //"serverSide": true,
	    "paging": false,
		"info": false,
        "columns": [
            {
				"render": function (data,type,row,meta) {
					var div = '<div class="div_center">'+(row["orderno"]+1)+'</div>';	
					return div;
				}
			},{
				width:40,
				"render": function (data,type,row,meta) {
					var name = row["name"];
					var div = '<div class="div_center">'+name+'</div>';	
					return div;
				}
			},{
				"render": function (data,type,row,meta) {
					var name = row["sumlx"];
					var div = '<div class="div_center">'+Math.round(name)+'</div>';	
					return div;
				}
			},{
				"render": function (data,type,row,meta) {
					var pfc = row["pfc"];
					if("全市" == row["name"]){
						qscflx[0] = pfc;
						}
					var div = '<div class="">'+(pfc)+'</div>';	
					return div;
				}
			},{
				"render": function (data,type,row,meta) {
					var qyc = row["qyc"];
					if("全市" == row["name"]){
						qscflx[1] = row["qyc"];
					}
					var div = '<div class="">'+(qyc)+'</div>';	
					return div;								
				}
			},{
				"render": function (data,type,row,meta) {
					var ltc = row["ltc"];
					if("全市" == row["name"]){
						qscflx[2] = row["ltc"];
					}
					var div = '<div class="">'+(ltc)+'</div>';	
					return div;								
				}
			},{
				"render": function (data,type,row,meta) {
					var lfc = row["lfc"];
					if("全市" == row["name"]){
						qscflx[3] = row["lfc"];
					}
					var div = '<div class="">'+(lfc)+'</div>';	
					return div;								
				}
			},{
				"render": function (data,type,row,meta) {
					var clzp = row["clzp"];
					if("全市" == row["name"]){
						qscflx[4] = row["clzp"];
					}
					var div = '<div class="">'+(clzp)+'</div>';	
					return div;					
				}
			},{
				"render": function (data,type,row,meta) {
					var cjzp = row["cjzp"];
					if("全市" == row["name"]){
						qscflx[5] = row["cjzp"];
					}
					var div = '<div class="">'+(cjzp)+'</div>';	
					return div;					
				}
			},{
				"render": function (data,type,row,meta) {
					var tlzp = row["tlzp"];
					if("全市" == row["name"]){
						qscflx[6] = row["tlzp"];
					}
					var div = '<div class="">'+(tlzp)+'</div>';	
					return div;					
				}
			},{
				"render": function (data,type,row,meta) {
					var qtjyc = row["qtjyc"];
					if("全市" == row["name"]){
						qscflx[7] = row["qtjyc"];
					}
					var div = '<div class="">'+(qtjyc)+'</div>';	
					return div;					
				}
			},{
				"render": function (data,type,row,meta) {
					var youguan = row["youguan"];
					if("全市" == row["name"]){
						qscflx[8] = row["youguan"];
					}
					var div = '<div class="">'+(youguan)+'</div>';	
					return div;					
				}
			}
			]
	});
	$('#cflx').on( 'draw.dt', function () {
		var qscf = [{name:"平房仓",value:qscflx[0]},{name:"浅圆仓",value:qscflx[1]},{name:"立筒仓",value:qscflx[2]},
		            {name:"楼房仓",value:qscflx[3]},{name:"储粮罩棚",value:qscflx[4]},{name:"仓间罩棚",value:qscflx[5]},
		            {name:"铁路罩棚",value:qscflx[6]},{name:"其他简易仓",value:qscflx[7]},{name:"其他",value:qscflx[8]}];
		//仓房状态饼状图
		option2 = {
			    title : {
			        text: '全市仓房类型统计',
			    },
			    tooltip : {
			        trigger: 'item',
			        formatter: "{a} <br/>{b} : {c}吨({d}%)",
			        position: function (p) {
		                // 位置回调
		                return [p[0]-190, p[1]+15];
		            }
			    },
			    legend: {
			    	 orient : 'vertical',
				        y:'center',
				        x:'left',
			        data:cflxmc
